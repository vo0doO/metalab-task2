mixin head(appleTouchIconPrecomposed=false, msapplicationTileImage=false,  icon192x192=false, dnsPrefetch=false, twitterCard=false, twitterSite=false, twitterImage=false, ogImage=false,ogUrl=false, canonical=false, title=false, description=false, keywords=false, themeColor=false,  titleColor=false,  safariPinnedTabColor=false, ...items)
	-
		var title = title ? title : 'Page title'
		var description = description ? description : 'Page description'
		var keywords = keywords ? keywords : 'Page keywords'
		var themeColor = keywords ? keywords : 'Page keywords'
		var titleColor = keywords ? keywords : 'Title color'
		var safariPinnedTabColor = keywords ? keywords : 'Safari pined message color'

	head
		meta(charset='UTF-8')
		meta(name='viewport' content='width=device-width, initial-scale=1.0')
		meta(http-equiv='X-UA-Compatible' content='ie=edge')
		meta(name='description' content=description)
		meta(name='keywords' content=keywords)
		meta(name='msapplication-TileColor' content="#3372DF")
		meta(name='theme-color' content="FIX_ME!!!")
		link(sizes='180x180' rel='apple-touch-icon' href='assets/favicons/apple-touch-icon.png')
		link(type='image/png' sizes='32x32' rel='icon' href='assets/favicons/favicon-32x32.png')
		link(type='image/png' sizes='16x16' rel='icon' href='assets/favicons/favicon-16x16.png')
		link(rel='manifest' href='assets/favicons/site.webmanifest')
		link(rel='mask-icon' href='assets/favicons/safari-pinned-tab.svg' color= "FIX_ME!!!")
		<!-- Add to homescreen for Chrome on Android -->
		meta(name="mobile-web-app-capable" content="yes")
		link(rel="icon" sizes="192x192" href="FIX_ME!!!")
		<!-- Add to homescreen for Safari on iOS --)
		meta(name="apple-mobile-web-app-capable" content="yes")
		meta(name="apple-mobile-web-app-status-bar-style" content="black")
		meta(name="apple-mobile-web-app-title" content="FIX_ME!!!")
		link(rel="apple-touch-icon-precomposed" href="FIX_ME!!!")
		<!-- Tile icon for Win8 (144x144 + tile color) -->
		meta(name="msapplication-TileImage" content="FIX_ME!!!-FIX_ME!!!)"
			meta(name="msapplication-TileColor" content="#3372DF")
			link(id="canonical" rel="canonical" href="FIX_ME!!!!")
		link(rel="dns-prefetch" href="FIX_ME!!!!")
		meta(name="twitter:card" content="FIX_ME!!!!")
		meta(name="twitter:site" content="FIX_ME!!!!")
		meta(name="twitter:title" content=title)
		meta(name="twitter:description" content=description)
		meta(name="twitter:image" content="FIX_MEE!!!")
		meta(property="og:image" content="FIX_MEE!!!" "thumbnailUrl")
		meta(property="og:title" content=title)
		meta(property="og:url" content="FIX_MEE!!!")
		meta(property="og:site_name" content=title)
		meta(property="og:description" content=description)
		// TODO: Добавить facebook, vk, metrika, ga, gtm,
		title=  title
		block content
