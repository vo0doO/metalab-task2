{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": ".pug format",
            "type": "process",
            "command": "${cwd}/node_modules/.bin/prettier.cmd --write --config=${cwd}/.prettierrc.cjs */**/**/**/.pug",
            "problemMatcher": [
                "$go"
            ]
        }
    ]
}